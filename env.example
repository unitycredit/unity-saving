## AWS S3 (S3 File Manager)
AWS_REGION=us-east-1
AWS_BUCKET_NAME=your-bucket-name

# Standard AWS names (recommended)
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# Alternative names also supported (optional)
# AWS_ACCESS_KEY=your_access_key_id
# AWS_SECRET_KEY=your_secret_access_key

# Optional prefix in bucket (keeps app objects in a folder-like prefix)
# UNITY_S3_PREFIX=unity-saving

## Future: AWS Cognito placeholders
# COGNITO_USER_POOL_ID=
# COGNITO_USER_POOL_CLIENT_ID=
# COGNITO_REGION=


